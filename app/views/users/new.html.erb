New User
<form class="" action="<%= users_url %>" method="post">
  <%= auth_token %>
  <label>
    username
    <input type="text" name="user[username]" value="<%=@user.username %>">
  </label>

  <label>
    password
    <input type="password" name="user[password]" value="<%=@user.password %>">
  </label>
  <input type="submit" name="name" value="Create User">
</form>
