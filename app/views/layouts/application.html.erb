<!DOCTYPE html>
<html>

<head>
  <title>NinetyNineCatsDay1</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if !current_user.nil? %>
    <ul>
      <li>Logged in as: <%= current_user.user_name %></li>
      <li> <%= button_to "Logout", session_url, method: :delete %></li>
    </ul>
  <% else %>
    <%= button_to "Login", new_session_url, method: :get %>
  <% end %>


<%= yield %>

</body>
</html>
